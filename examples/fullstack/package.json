    "@types/node": "^24.3.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.11.6"
  },
  "dependencies": {
    "@open-rpc/client-js": "^1.8.1",
    "@open-rpc/meta-schema": "^1.14.9",
    "@open-rpc/schema-utils-js": "^2.1.2",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "solid-js": "^1.9.5"
  }
}
