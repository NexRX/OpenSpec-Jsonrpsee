{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite",
    "gen:rpc": "npm run gen:rpc:generate && npm run gen:rpc:move",
    "gen:rpc:generate": "open-rpc-generator generate -t client -l typescript -n rpc-client -d ./spec.json -o ./",
    "gen:rpc:move": "npx shx cp client/typescript/src/index.ts ./src/rpc-client.ts",
    "serve": "vite preview"
  },
